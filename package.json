{
  "name": "foo",
  "version": "0.0.1",
  "description": "Foo",
  "keywords": [
    "react",
    "react.js",
    "vue",
    "vue.js"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc-plus -b tsconfig.src.json",
    "watch": "vue-tsc-plus -b --watch",
    "type-check": "vue-tsc-plus --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublish": "pnpm build"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/vue": "^8.1.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.17",
    "@vue/test-utils": "^2.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vitest": "^4.0.17",
    "vue-tsc-plus": "^0.0.1"
  }
}
